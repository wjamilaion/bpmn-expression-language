{
  "parserOptions": { 
    "ecmaVersion": 2019 
  },
  "extends": [
    "plugin:bpmn-io/browser",
    "plugin:bpmn-io/jsx",
    "plugin:bpmn-io/node",
    "plugin:react-hooks/recommended"
  ],
  "rules": {
    "import/first": "error",
    "import/no-amd": "error",
    "import/no-webpack-loader-syntax": "error"
  },
  "plugins": [
    "import"
  ]
}
